{
  "name": "slow_rate_limit_demo",
  "description": "Deliberately slow rate limiter highlighting throttling behaviour",
  "enabled": true,
  "temperature": 0.0,
  "max_tokens": 256,
  "prompt_pack": "sample",
  "rate_limiter": {
    "plugin": "fixed_window",
    "security_level": "official",
    "options": {
      "requests": 1,
      "per_seconds": 5
    }
  },
  "cost_tracker": {
    "plugin": "fixed_price",
    "security_level": "official",
    "options": {
      "prompt_token_price": 0.0001,
      "completion_token_price": 0.00015
    }
  },
  "concurrency": {
    "enabled": false
  },
  "baseline_plugins": [
    {
      "name": "score_delta",
      "security_level": "official"
    }
  ]
}
