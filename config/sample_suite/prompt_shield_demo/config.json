{
  "name": "prompt_shield_demo",
  "description": "Showcase prompt shield masking behaviour for sensitive terms.",
  "enabled": true,
  "temperature": 0.0,
  "max_tokens": 256,
  "prompt_pack": "sample",
  "llm_middlewares": [
    {
      "name": "prompt_shield",
      "determinism_level": "guaranteed",
      "options": {
        "denied_terms": ["classified", "top secret"],
        "on_violation": "mask",
        "mask": "[REDACTED]"
      }
    }
  ]
}
